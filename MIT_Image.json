  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "type": "string"
    },
    "vmSize": {
      "type": "string",
      "defaultValue": "Standard_DS1"
    },
    "adminUserName": {
      "type": "string"
    },
    "adminPassword": {
      "type": "securestring"
    },
    "networkInterfaceId": {
      "type": "string"
    }
  },
  "resources": [
    {
      "apiVersion": "2016-03-30",
      "properties": {
        "hardwareProfile": {
          "vmSize": "[parameters('vmSize')]"
        },
        "storageProfile": {
          "osDisk": {
            "osType": "Linux",
            "name": "MIT-osDisk.23543dac-a16c-465d-93e3-cf5cace40eb9.vhd",
            "createOption": "FromImage",
            "image": {
              "uri": "https://sto4ob3komixuz3.blob.core.windows.net/system/Microsoft.Compute/Images/vhds/MIT-osDisk.23543dac-a16c-465d-93e3-cf5cace40eb9.vhd"
            },
            "vhd": {
              "uri": "https://sto4ob3komixuz3.blob.core.windows.net/vmcontainer84c21b07-e95c-4152-963b-b979a5be012d/osDisk.84c21b07-e95c-4152-963b-b979a5be012d.vhd"
